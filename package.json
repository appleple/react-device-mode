{
  "name": "react-device-mode",
  "version": "1.4.1",
  "description": "device viewer for a-blog cms",
  "type": "module",
  "browserslist": [
    "defaults",
    "safari >= 12"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"tsup --watch --sourcemap inline\" \"vite\"",
    "lint": "eslint \"src/**/*.{js,mjs,cjs,ts,jsx,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,mjs,cjs,ts,jsx,tsx}\"",
    "deploy": "np --yolo",
    "prepare": "husky",
    "prepack": "npm run build"
  },
  "author": "steelydylan",
  "license": "MIT",
  "dependencies": {
    "re-resizable": "^6.9.17",
    "styled-components": "^6.1.12"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0",
    "react-dom": ">=16.8.0 <19.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.7.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "np": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.1.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "vite": "^5.3.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "overrides": {
    "eslint": "^9.7.0"
  }
}
